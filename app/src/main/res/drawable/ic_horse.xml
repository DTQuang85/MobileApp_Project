<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Body -->
    <item android:left="10dp" android:top="12dp">
        <shape android:shape="oval">
            <size android:width="18dp" android:height="10dp"/>
            <solid android:color="#FFFFFF"/>
        </shape>
    </item>
    <!-- Head -->
    <item android:left="2dp" android:top="10dp">
        <shape android:shape="oval">
            <size android:width="10dp" android:height="8dp"/>
            <solid android:color="#FFFFFF"/>
        </shape>
    </item>
    <!-- Ear -->
    <item android:left="4dp" android:top="4dp">
        <shape android:shape="rectangle">
            <size android:width="4dp" android:height="6dp"/>
            <solid android:color="#FFFFFF"/>
        </shape>
    </item>
    <!-- Neck -->
    <item android:left="7dp" android:top="10dp">
        <shape android:shape="rectangle">
            <size android:width="6dp" android:height="8dp"/>
            <solid android:color="#FFFFFF"/>
        </shape>
    </item>
    <!-- Legs -->
    <item android:left="12dp" android:top="20dp">
        <shape android:shape="rectangle">
            <size android:width="3dp" android:height="8dp"/>
            <solid android:color="#FFFFFF"/>
        </shape>
    </item>
    <item android:left="18dp" android:top="20dp">
        <shape android:shape="rectangle">
            <size android:width="3dp" android:height="8dp"/>
            <solid android:color="#FFFFFF"/>
        </shape>
    </item>
    <!-- Tail -->
    <item android:left="26dp" android:top="14dp">
        <shape android:shape="rectangle">
            <size android:width="4dp" android:height="6dp"/>
            <solid android:color="#FFFFFF"/>
        </shape>
    </item>
</layer-list>
